<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>READ</title>
</head>
<!-- bootstrap 4  -->
<!-- Latest compiled and minified CSS -->
<link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.6.2/dist/css/bootstrap.min.css">

<!-- jQuery library -->
<script src="https://cdn.jsdelivr.net/npm/jquery@3.6.4/dist/jquery.slim.min.js"></script>

<!-- Popper JS -->
<script src="https://cdn.jsdelivr.net/npm/popper.js@1.16.1/dist/umd/popper.min.js"></script>

<!-- Latest compiled JavaScript -->
<script src="https://cdn.jsdelivr.net/npm/bootstrap@4.6.2/dist/js/bootstrap.bundle.min.js"></script>
<style>
    table, th, td {
      border:1px solid black;
    }
    </style>
<body>
    <table style="width:100%">
        <tr>
          <th>Email</th>
          <th>Full Name</th>
          <th>Address</th>
          <th>Phone Number</th>
          <th>Action</th>

        </tr>
        <% for(let i=0; i < dataTable.length; i++) { %>
            <tr>
              <td><%= dataTable[i].email %></td>
              <td><%= dataTable[i].fullName %></td>
              <td><%= dataTable[i].address %></td>
              <td><%= dataTable[i].phoneNumber %></td>
              <td>
                <a href="/edit-crud?id=<%=dataTable[i].id%>" class="btn btn-warning" >Edit</a>
                <a href="/delete-crud?id=<%=dataTable[i].id%>" class="btn btn-danger">Delete</a>
              </td>
            </tr>
         <% } %>
      </table>
      
</body>
</html>